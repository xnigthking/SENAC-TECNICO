package aula06_22_07_2025;

public class Animal {
	String nome;
	int idade;
	double peso;
	
	
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public int getIdade() {
		return idade;
	}
	public void setIdade(int idade) {
		this.idade = idade;
	}
	public double getPeso() {
		return peso;
	}
	public void setPeso(double peso) {
		this.peso = peso;
	}
		
	public abstract void emitirSom(); //MÃ©todo abstrato nao possui corpo
	
}
