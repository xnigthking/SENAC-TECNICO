package aula02_15_07_2025;

public class Carro {
	//Atributos
	String marca;
	String modelo;
	String cor;
	String potencia;
	String placa;
	String chassi;
	double preco;
	int portas;
	
	//MÃ©todos 
	public void exibirDetalhes () {
		System.out.println("Marca: +marca");
		System.out.println("Modelo: +modelo");
		System.out.println("Cor: +cor");
	}
}
